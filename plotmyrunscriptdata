#! /usr/bin/gnuplot

set title 'Temperatures'
set timefmt '%Y-%m-%d %H:%M'
set xdata time
set datafile sep ','
set xlabel 'Date/Time'
set ylabel 'Temperature/Â°C'
set grid
set style data lines

